{
  "plugins": [
    "@babel/plugin-proposal-class-properties"
  ],
  "presets": [
    ["@babel/preset-env", {
      "corejs": 2,
      "modules": false,
      "targets": {
        "node": "6.10",
        "browsers": [
          "Firefox > 20",
          "chrome > 30"
        ]
      },
      "useBuiltIns": "usage"
    }],
    "@babel/preset-react",
    "@babel/preset-typescript"
  ]
}
