{
  "plugins": [
    "@babel/plugin-proposal-class-properties"
  ],
  "presets": [
    ["@babel/preset-env", {
      "modules": false,
      "targets": {
        "node": "6.10",
        "browsers": [
          "Firefox > 20",
          "chrome > 30"
        ]
      },
      "corejs": 2,
      "useBuiltIns": "usage"
    }],
    "@babel/preset-react"
  ]
}
