{
  "name": "@datawheel/tesseract-explorer",
  "version": "0.5.3",
  "description": "A react component to explore data from a tesseract-olap server.",
  "main": "dist/explorer.cjs.js",
  "module": "dist/explorer.esm.js",
  "files": [
    "dist/explorer.cjs.js",
    "dist/explorer.esm.js",
    "dist/explorer.css"
  ],
  "scripts": {
    "dev": "multiview [npm run dev:watch] [npm run dev:serve] -x 2000",
    "dev:serve": "poi --serve ./dev/index.js",
    "dev:watch": "rollup --config rollup.config.js --environment NODE_ENV:development,BABEL_ENV:development --watch",
    "prepublishOnly": "npm run build",
    "build": "rollup --config rollup.config.js --environment NODE_ENV:production,BABEL_ENV:production"
  },
  "author": "Francisco Abarz√∫a",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Datawheel/tesseract-ui.git",
    "directory": "packages/tesseract-explorer"
  },
  "keywords": [
    "tesseract-explorer",
    "tesseract-olap",
    "tesseract-ui"
  ],
  "dependencies": {
    "@babel/polyfill": "^7.6.0",
    "@datawheel/olap-client": "^1.0.11",
    "classnames": "^2.2.6",
    "clipboard-copy": "^3.0.0",
    "form-urldecoded": "^0.0.6",
    "form-urlencoded": "^4.0.0",
    "memoize-one": "^5.0.5",
    "pluralize": "^8.0.0",
    "react-perfect-scrollbar": "^1.5.3",
    "reselect": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "@blueprintjs/core": "^3.18.1",
    "@blueprintjs/select": "^3.8.0",
    "@blueprintjs/table": "^3.7.1",
    "autoprefixer": "^9.6.0",
    "eslint": "^5.16.0",
    "multiview": "^2.5.3",
    "poi": "^12.6.10",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.12.6",
    "react-redux": "^7.0.3",
    "redux": "^4.0.4",
    "rollup": "^1.21.2",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.0.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-replace": "^2.2.0",
    "sass": "^1.22.10"
  },
  "peerDependencies": {
    "@blueprintjs/core": "^3.15.1",
    "@blueprintjs/select": "^3.8.0",
    "@blueprintjs/table": "^3.5.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.3",
    "redux": "^4.0.1"
  }
}
